            <input type="email" name="email"id="input_email" placeholder="Digite seu Email"required>
        </div>
        <div>
            <label for="senha">Senha: </label>
            <input type="password" name="senha" id="input_senha" placeholder="Digite ssua Senha"required>
        </div>
        <div>
            <label for="endereco">Endere√ßo: </label>
            <input type="text" name="endereco"id="input_endereco" placeholder="Digite seu endereco"required>
        </div>
        <button type= "submit">Adicionar usuario</button>
    </form>
    
</body>
</html>